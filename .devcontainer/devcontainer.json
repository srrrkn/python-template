{
    "name": "myservice-python",
    "build": {
        "context": "..",
        "dockerfile": "../builds/Dockerfile"
    },
    "workspaceFolder": "/workspace",
    "mounts": [
        "source=${localWorkspaceFolder},target=/workspace,type=bind"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.debugpy",
                "charliermarsh.ruff"
            ]
        }
    }
}
